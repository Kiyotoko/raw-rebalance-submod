	<!-- The Underworld-specific version of the Racketeering corruption upgrade. -->
	<!-- This one gives the "Salvage" effect to the Underworld, granting them credits -->
	<!-- for destroying enemy units. -->
	<UpgradeObject Name="Empire_Racketeering_Corruption_Upgrade">
		<Variant_Of_Existing_Type>Generic_Racketeering_Corruption_Upgrade</Variant_Of_Existing_Type>
		<Affiliation>Empire</Affiliation>
		<Abilities SubObjectList="Yes">
			<Bounty_On_Faction_Ability Name="Underworld_Racketeering_Salvage_Effect">
				<Bounty_Percentage_Amount> 1.0 </Bounty_Percentage_Amount>
				<Stacking_Category> 1 </Stacking_Category>
				<!-- The Underworld player will gain credits for destroying objects owned by these factions: -->
				<Target_Faction_Names>Rebel, Hutts, Pirates </Target_Faction_Names>
				<Apply_To_All_Allies> No </Apply_To_All_Allies>
			</Bounty_On_Faction_Ability>
		</Abilities>
	</UpgradeObject>
